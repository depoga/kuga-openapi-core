{
    "name":"kuga/openapi-core",
    "description":"kuga核心SDK，在https://github.com/misnet/kuga-openapi-sdk 上进行了简化，去掉业务相关的逻辑，只保留框架层级相关接口",
    "type":"library",
    "license": "LGPL-3.0",
    "require":{
        "php":">=7.0",
        "kuga/qing":"dev-master",
        "phalcon/incubator": "^3.4",
        "aliyuncs/oss-sdk-php": "~2.0",
        "qcloudsms/qcloudsms_php": "0.1.*",
        "alibabacloud/client":">=1.0.7",
        "phpoffice/phpspreadsheet": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Kuga\\": "src"
        }
    },
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "https://packagist.phpcomposer.com"
        },
        "qing": {
            "type": "vcs",
            "url": "https://github.com/misnet/qing.git"
        }
    }
}